<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="RoomListItemRendererSkin" states="up,down,disabled" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing"
        height="50" width="200">
	<e:Image width="100%" height="100%" scale9Grid="1,3,8,8" alpha.disabled="0.5" source="button_up_png"
	         source.down="button_down_png" />
	<e:Label id="labelDisplay" top="8" bottom="8" left="8" right="8" size="20" fontFamily="Tahoma" textColor="0xFFFFFF"
	         verticalAlign="middle" textAlign="center"
	         text="{data.roomID+'   ('+data.playerCount+'/'+data.maxPlayerCount+')'}" />
</e:Skin>